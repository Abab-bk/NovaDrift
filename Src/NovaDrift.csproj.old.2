<Project Sdk="Godot.NET.Sdk/4.3.0-dev.6">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <TargetFramework Condition=" '$(GodotTargetPlatform)' == 'android' ">net8.0</TargetFramework>
    <TargetFramework Condition=" '$(GodotTargetPlatform)' == 'ios' ">net8.0</TargetFramework>
    <EnableDynamicLoading>true</EnableDynamicLoading>
    <LangVersion>12.0</LangVersion>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>
  <ItemGroup>
    <Folder Include="Scripts\Systems\Shields\" />
    <Folder Include="Scripts\Systems\Weapons\" />
    <Compile Remove="script_templates/**/*.cs" />
    <Compile Remove=".godot\**" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="addons\AcidUtilities\plugin.cfg" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Remove=".godot\**" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="GDTask" Version="1.2.2" />
    <PackageReference Include="GodotSharpSome.Drawing2D" Version="0.23.0" />
    <ProjectReference Include="..\GodotGetNode\GodotGetNode.csproj"
                      ReferenceOutputAssembly="false"
                      OutputItemType="Analyzer"/>
  </ItemGroup>
</Project>